{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://your-bucks.vercel.app/api/:path*"
      }
    ]
  }